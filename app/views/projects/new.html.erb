
<div class='well'>
  <% form_for @project do |f| %>
    <div class='control-group'>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class='control-group'>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <%= f.submit 'Create Project', class: 'btn btn-primary' %>
  <% end %>
</div>