<table class="table table-cell-padding">
  <thead>
    <tr>
      <th>Name</th>
      <th>Assignee</th>
      <th>Status</th>
      <th>Tags</th> 
    </tr>
  </thead>
  <tbody>
    <% ticket_obj.each do |ticket| %>
      <tr>
        <td>
          <%= link_to ticket.name, ticket_path(ticket.id) %>
        </td>
        <td>
          <%= %> 
        </td>
        <td>
          <%= ticket.status %>
        <td>
        <td>
          <%# <%= ticket.tags %>
        </td>
        <td>
          <%= link_to 'Edit', edit_ticket_path(ticket.id), class: 'd-inline-block' %>
          <%= link_to 'Destroy', project_path(ticket.id), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>